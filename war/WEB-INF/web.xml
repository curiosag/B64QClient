<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

	<servlet>
		<servlet-name>codecServlet</servlet-name>
		<servlet-class>fusionTableClient.server.CodecServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>codecServlet</servlet-name>
		<url-pattern>/fusiontableclient/codec</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>FusionTableClient.html</welcome-file>
	</welcome-file-list>

</web-app>
